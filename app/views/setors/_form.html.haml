= form_for @setor do |f|
  - if @setor.errors.any?
    #error_explanation
      %h2= "#{pluralize(@setor.errors.count, "error")} prohibited this setor from being saved:"
      %ul
        - @setor.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :nome
    = f.text_field :nome
  .actions
    = f.submit 'Salvar'
